/* Finding the position of kth missing element of the array */

#include <iostream>

using namespace std;

int main()
{
    
   int a=0;
   int k=0;
   cin>>k;
   
   int arr[5] = {2,3,4,7,11};
   int temp[a];
   
   while(a<k)
   {
       for(int i=0;i< arr[4];i++)
       {
           if(i!=arr[i])
           {
               temp[a]=i;
               a++;
           }
       }
   }
   
  cout<< temp[a];

    return 0;
}
