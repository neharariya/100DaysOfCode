/* BUBBLE SORT ALGORITHM */

#include <iostream>
#include<climits>

using namespace std;

int main()
{
    
    int i,j=0;
    
    int arr[5]= {7,4,8,5,3};
    
    for(i=0; i<4;i++)
    {
        for(j=0; j<4-i;j++)
        {
            if(arr[j]>arr[j+1])
            {
                swap(arr[j],arr[j+1]);
            }
        }
    }

    for(i=0; i<5;i++)
    {
        cout<<arr[i];
    }
    return 0;
}
